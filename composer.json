{
    "name": "navindbhudiya/module-product-recommendation",
    "description": "AI-Powered Product Recommendations using ChromaDB Vector Database for Magento 2",
    "keywords": [
        "magento2",
        "magento 2",
        "ai",
        "product recommendation",
        "chromadb",
        "vector database",
        "embeddings",
        "machine learning",
        "related products",
        "cross-sell",
        "up-sell",
        "personalization",
        "llm",
        "claude",
        "openai"
    ],
    "type": "magento2-module",
    "version": "1.0.0",
    "license": "MIT",
    "homepage": "https://github.com/navindbhudiya-aws/magento2-ai-product-recommendations",
    "authors": [
        {
            "name": "Navin Bhudiya",
            "email": "navindbhudiya@gmail.com",
            "role": "Magento 4x Certified | AWS 2x Certified | Cloud Solutions Architect | Transforming E-commerce with Cloud Innovation",
            "homepage": "https://github.com/navindbhudiya-aws"
        }
    ],
    "require": {
        "php": "^8.1 || ^8.2 || ^8.3",
        "ext-json": "*",
        "ext-curl": "*",
        "ext-openssl": "*",
        "magento/framework": "^103.0",
        "magento/module-catalog": "^104.0",
        "magento/module-store": "^101.1",
        "magento/module-config": "^101.2",
        "magento/module-eav": "^102.1",
        "magento/module-customer": "^103.0",
        "magento/module-sales": "^103.0",
        "magento/module-wishlist": "^101.2",
        "magento/module-widget": "^101.2",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "magento/magento-coding-standard": "^33",
        "phpstan/phpstan": "^1.10",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "suggest": {
        "magento/module-catalog-widget": "Required for recommendation widgets",
        "magento/module-reports": "Enhanced tracking and analytics",
        "magento/module-customer-segment": "Advanced customer segmentation"
    },
    "autoload": {
        "files": [
            "registration.php"
        ],
        "psr-4": {
            "NavinDBhudiya\\ProductRecommendation\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NavinDBhudiya\\ProductRecommendation\\Test\\": "Test/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "magento/composer-dependency-version-audit-plugin": true,
            "magento/inventory-composer-installer": true,
            "magento/composer-root-update-plugin": true
        },
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "extra": {
        "magento-root-dir": "./",
        "magento-force": true
    },
    "support": {
        "issues": "https://github.com/navindbhudiya/magento2-product-recommendation/issues",
        "source": "https://github.com/navindbhudiya/magento2-product-recommendation",
        "docs": "https://github.com/navindbhudiya/magento2-product-recommendation/wiki",
        "email": "navindbhudiya@gmail.com"
    },
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/navindbhudiya"
        }
    ]
}
